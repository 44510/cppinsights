  int __suspend_index;
  bool __initial_await_suspend_called;
  
  struct S
  {
    int t;
    char c;
  };
  
  public: 
  int start;
  int i;
  S s;
  std::suspend_always __suspend_42_11;
  std::suspend_always __suspend_49_5;
  std::suspend_always __suspend_50_12;
  std::suspend_always __suspend_51_5;
  std::suspend_always __suspend_42_11_1;
};

generator seq(int start)
{
  /* Allocate the frame including the promise */
